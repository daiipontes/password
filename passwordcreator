import random

input('Digite o seu nome para começar\n')

lower = "abcdfghijklmnopqrstuvwxyz"
upper = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
numbers = "0123456789"
symbols = "@#!$&*:;?"

all = lower + upper + numbers + symbols 
length = 10
password = "".join(random.sample(all, length))


print('Aqui está a sua senha')
print(password)

